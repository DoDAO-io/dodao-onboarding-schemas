{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "commonjs",
    "lib": ["esnext"],
    "rootDir": "./",
    "outDir": "./build",
    "esModuleInterop": true,
    "paths": {
      "models/*": ["src/models/*"],
      "input/*": ["src/input/*"]
    },
    "strict": true,
    "noImplicitAny": false,
    "resolveJsonModule": true
  }
}
